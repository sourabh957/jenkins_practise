
properties([parameters([choice(choices: ['add', 'sub', 'mul', 'div'], name: 'function'), string(defaultValue: '0', name: 'a', trim: true), string(defaultValue: '1', name: 'b')])])

pipeline {
    agent any
    stages {
        stage('checkout') {
            steps {
                checkout([$class: 'GitSCM', branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[credentialsId: 'fdb670e1-cb4a-499f-a67d-37368db1ba5e', url: 'https://github.com/sourabh957/jenkins_practise.git']]])
            }
        }

        stage('build') {
            steps {
                git branch: 'main', credentialsId: 'fdb670e1-cb4a-499f-a67d-37368db1ba5e', url: 'https://github.com/sourabh957/jenkins_practise.git'
                def tempa = ${params.a} - '0'
                tempa = (int) temp
                def tempb = ${params.b} - '0'
                tempb = (int) tempb
                bat "python ${params.function}.py ${params.function} tempa tempb"
            }
        }

        stage('test') {
            steps {
                echo 'In the testing stage'
            }
        }

    }
    
}
